		<![CDATA[SELECT "PROJETOS"."NOME" AS PROJETO,
	"FUNCIONARIOS"."NOME" AS FUNCIONARIO,
	"FUNCIONARIOS"."TIPO" AS CARGO,
	"FUNCIONARIOS"."VALOR_HORA" AS TAXA
